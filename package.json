  "license": "ISC",
  "devDependencies": {
    "@parcel/transformer-pug": "^2.9.3",
    "parcel": "^2.9.3",
    "parcel-reporter-static-files-copy": "^1.5.2",
    "push-dir": "^0.4.1"
  },
  "staticFiles": {
    "staticPath": "src/img"
  }
}
