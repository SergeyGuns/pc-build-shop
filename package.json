{
  "name": "pc-build-shop",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "rm -fr .parcel-cache/ && parcel src/index.pug",
    "build-dev": "parcel build ./src/index.html --no-optimize --public-url ./",
