        link( href="https://fonts.googleapis.com/css2?family=Major+Mono+Display&family=Montserrat:wght@300;400;500;600;700&display=swap" rel="stylesheet")
        link(href="https://fonts.googleapis.com/css2?family=Roboto:wght@500;600;700&family=Major+Mono+Display&family=Montserrat:wght@400;700&display=swap" rel="stylesheet")
        link(rel="stylesheet" href="/src/styles/style.css")
        meta(charset="UTF-8")
        meta(name="viewport" content="width=device-width, initial-scale=1.0")
        script. 
            const MAIN_COLORS = [ 'rgba(255,231,1,1)', 'rgba(154,21,236,1)']
        title= title